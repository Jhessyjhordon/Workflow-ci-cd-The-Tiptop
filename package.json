{
  "name": "the-tiptop-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:unit": "ng test --no-watch --no-progress --include='**/*.unit.spec.ts' --browsers=ChromeHeadless --karma-config=karma.conf.js",
    "test:integration": "ng test --no-watch --no-progress --include='**/*.integration.spec.ts' --browsers=ChromeHeadless --karma-config=karma.conf.js",
    "dev:ssr": "ng run the-tiptop-front:serve-ssr",
    "serve:ssr": "node dist/the-tiptop-front/server/main.js",
    "build:ssr": "ng build && ng run the-tiptop-front:server",
    "prerender": "ng run the-tiptop-front:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@nguniversal/express-engine": "^16.2.0",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "express": "^4.15.2",
    "jwt-decode": "^4.0.0",
    "ngx-bootstrap": "^11.0.2",
    "puppeteer": "^21.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@types/bootstrap": "^5.2.7",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-typescript": "^5.5.4",
    "karma-verbose-reporter": "^0.0.8",
    "typescript": "~5.1.3"
  }
}